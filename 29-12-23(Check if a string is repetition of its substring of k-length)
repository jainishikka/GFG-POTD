class Solution
{
    int kSubstrConcat(int n, String s, int k)
    {
        if(n%k!=0)
        return 0;
        
        HashMap<String,Integer> map=new HashMap<>();
        for(int i=0;i<n;i+=k){
            String sub=s.substring(i,i+k);
            map.put(sub,map.getOrDefault(sub,0)+1);
        }
        if(map.size()>2)
        return 0;
        return 1;
    }
}
